{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/home", "dest": "/home.html" },
    { "src": "/subscription", "dest": "/subscription.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
